{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "coredns": {
            "type": "object",
            "properties": {
                "deployment": {
                    "type": "object",
                    "properties": {
                        "skipConfig": {
                            "type": "boolean"
                        }
                    }
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "isClusterService": {
                    "type": "boolean"
                },
                "serviceAccount": {
                    "type": "object",
                    "properties": {
                        "create": {
                            "type": "boolean"
                        },
                        "name": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "externaldns": {
            "type": "object",
            "properties": {
                "image": {
                    "type": "string"
                },
                "interval": {
                    "type": "string"
                },
                "securityContext": {
                    "type": "object",
                    "properties": {
                        "fsGroup": {
                            "type": "integer"
                        },
                        "runAsNonRoot": {
                            "type": "boolean"
                        },
                        "runAsUser": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "global": {
            "type": "object",
            "properties": {
                "imagePullSecrets": {
                    "type": "array"
                }
            }
        },
        "infoblox": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "gridHost": {
                    "type": "string"
                },
                "httpPoolConnections": {
                    "type": "integer"
                },
                "httpRequestTimeout": {
                    "type": "integer"
                },
                "sslVerify": {
                    "type": "boolean"
                },
                "wapiPort": {
                    "type": "integer"
                },
                "wapiVersion": {
                    "type": "string"
                }
            }
        },
        "k8gb": {
            "type": "object",
            "properties": {
                "clusterGeoTag": {
                    "type": "string"
                },
                "coredns": {
                    "type": "object"
                },
                "deployCrds": {
                    "type": "boolean"
                },
                "deployRbac": {
                    "type": "boolean"
                },
                "dnsZone": {
                    "type": "string"
                },
                "dnsZoneNegTTL": {
                    "type": "integer"
                },
                "edgeDNSServers": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "edgeDNSZone": {
                    "type": "string"
                },
                "extGslbClustersGeoTags": {
                    "type": "string"
                },
                "imageRepo": {
                    "type": "string"
                },
                "imageTag": {
                    "type": "null"
                },
                "log": {
                    "type": "object",
                    "properties": {
                        "format": {
                            "type": "string"
                        },
                        "level": {
                            "type": "string"
                        }
                    }
                },
                "metricsAddress": {
                    "type": "string"
                },
                "reconcileRequeueSeconds": {
                    "type": "integer"
                },
                "securityContext": {
                    "type": "object",
                    "properties": {
                        "allowPrivilegeEscalation": {
                            "type": "boolean"
                        },
                        "readOnlyRootFilesystem": {
                            "type": "boolean"
                        },
                        "runAsNonRoot": {
                            "type": "boolean"
                        },
                        "runAsUser": {
                            "type": "integer"
                        }
                    }
                },
                "splitBrainCheck": {
                    "type": "boolean"
                }
            }
        },
        "ns1": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "ignoreSSL": {
                    "type": "boolean"
                }
            }
        },
        "openshift": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                }
            }
        },
        "rfc2136": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "rfc2136Opts": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "host": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "route53": {
            "type": "object",
            "properties": {
                "assumeRoleArn": {
                    "type": "null"
                },
                "enabled": {
                    "type": "boolean"
                },
                "hostedZoneID": {
                    "type": "string"
                },
                "irsaRole": {
                    "type": "string"
                }
            }
        },
        "tracing": {
            "type": "object",
            "properties": {
                "deployJaeger": {
                    "type": "boolean"
                },
                "enabled": {
                    "type": "boolean"
                },
                "endpoint": {
                    "type": "string"
                },
                "jaegerImage": {
                    "type": "object",
                    "properties": {
                        "pullPolicy": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "otelConfig": {
                    "type": "null"
                },
                "samplingRatio": {
                    "type": "null"
                },
                "sidecarImage": {
                    "type": "object",
                    "properties": {
                        "pullPolicy": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}
